<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>TemplateProject.Services</Name>
    <Description>N-Layared .Net Core WebAPI project template (Application)</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>TemplateProject.Services</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>__TemplateIcon.ico</Icon>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="TemplateProject.Services.csproj" File="TemplateProject.Services.csproj" ReplaceParameters="true">
      <Folder Name="Contracts" TargetFolderName="Contracts" />
      <Folder Name="Implementations" TargetFolderName="Implementations" />
    </Project>
  </TemplateContent>
</VSTemplate>